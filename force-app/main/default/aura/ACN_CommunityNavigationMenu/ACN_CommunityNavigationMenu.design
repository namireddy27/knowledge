<design:component>
	<design:attribute name="dataProviderName" label="Data Provider Name" description="Specifies the data provider for the component." datasource="apex://ACN_NavigationDataProviderDataSource" />
    <design:attribute name="isCacheEnabled" label="Is Data Provider Cache Enabled?" description="Use this property to specify if the data provider should try to use platform cache." />
    <design:attribute name="isDataLoadAsynchronous" label="Is Data Load Asynchronous?" description="Use this property to specify if the data provider should load data in asynchronously." />
    <design:attribute name="itemSize" label="Custom Navigation Menu Item Size" description="Use this attribute to specify the size of the custom navigation menu items (default is 2)." />
    <design:attribute name="styleClass" label="CSS Style Class Name" description="The CSS Style Class Name to be applied to the Component." datasource="apex://ACN_NavigationStyleClassDataSource"/>
</design:component>